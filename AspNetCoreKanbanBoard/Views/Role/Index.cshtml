@model AspNetCoreKanbanBoard.ViewModels.DisplayViewModel

@{ 


}

<div>
    <div>
        <h2>Role List</h2>
        <ul>
            @foreach (var role in Model.Roles)
            {
                <li>
                    @role
                </li>

            }
        </ul>
    </div>
    <partial name="_CreateRolePartial" model=" new AspNetCoreKanbanBoard.ViewModels.RoleViewModel()">
</div>
<div>
    <partial name="_AddUserToRolePartial" model="new AspNetCoreKanbanBoard.ViewModels.UpdateRoleViewModel() { Users =@Model.Users, Roles = @Model.Roles}">
</div>